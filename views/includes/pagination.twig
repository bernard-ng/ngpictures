<ul class="pagination center">
    <li class="prev">
        <a href="?page={{ next }}">
            <i class="icon icon-left-open"></i>
        </a>
    </li>
    <li class="active">
        <a href="?page=1">1</a>
    </li>
    {% for i in 1..5 %}
        <li>
            <a href="?page={{ i }}">{{ i }}</a>
        </li>
    {% endfor %}
    <li class="next">
        <a href="?page={{ prev }}">
            <i class="icon icon-right-open"></i>
        </a>
    </li>
</ul>
